@inherits System.Web.Mvc.WebViewPage<AdvancedExternalReviews.ContentPreviewModel>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
    <head>
        <title>@Model.Name (@Model.Token)</title>
        <meta name="robots" content="noindex">

        <style type="text/css">
            html, head, body {
                height: 100%;
                padding: 0;
                margin: 0;
                font-size: 0;
                line-height: 0;
            }

            iframe {
                padding: 0;
                margin: 0;
                border: none;
            }
        </style>
    </head>
    <body>
    <iframe src="/en/externalPageReview/@(Model.Token)" width="100%" height="100%"></iframe>
    </body>
</html>

