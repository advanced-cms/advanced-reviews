<?xml version="1.0" encoding="utf-8"?>
<module clientResourceRelativePath="1.0.0">

  <assemblies>
    <add assembly="AdvancedApprovalReviews" />
  </assemblies>

  <clientResources>
    <add name="AdvancedReviews" path="styles/styles.css" resourceType="Style" />
  </clientResources>

  <clientModule initializer="episerver-addons-review.initializer">
    <moduleDependencies>
      <add dependency="CMS" type="RunAfter" />
    </moduleDependencies>
    <requiredResources>
      <add name="AdvancedReviews" />
    </requiredResources>
  </clientModule>

  <dojo>
    <paths>
      <add name="episerver-addons-review" path="Scripts" />
    </paths>
 </dojo>
</module>