{
    "name": "episerver-ui",
    "version": "",
    "description": "",
    "scripts": {
        "build": "npm run build:widget && npm run build:admin && npm run build:external-review-manage-links-widget && npm run build:editable-external-component",
        "build:debug": "npm run build:widget:debug && npm run build:admin:debug && npm run build:external-review-manage-links-widget:debug && npm run build:editable-external-component:debug",
        "build:widget": "webpack --config webpack.widget.config.js --mode production",
        "build:widget:debug": "webpack --config webpack.widget.config.js --mode development",
        "build:admin": "webpack --config admin/webpack.admin.config.js --mode production",
        "build:admin:debug": "webpack --config admin/webpack.admin.config.js --mode development",
        "build:editable-external-component": "webpack --config editable-external-reviews/webpack.editable-external-review-component.config.js --mode production",
        "build:editable-external-component:debug": "webpack --config editable-external-reviews/webpack.editable-external-review-component.config.js --mode development",
        "build:external-review-manage-links-widget": "webpack --config external-reviews-manage-links/webpack.external-review-manage-links.widget.config --mode production",
        "build:external-review-manage-links-widget:debug": "webpack --config external-reviews-manage-links/webpack.external-review-manage-links.widget.config --mode development",
        "storybook": "start-storybook -s ./ -p 6006",
        "build-storybook": "build-storybook",
        "generate-resource-definitions": "node ./tools/resourceGenerator.js",
        "generate-resource-definitions-debug": "node --nolazy --inspect-brk=9229 ./tools/resourceGenerator.js"
    },
    "author": "Episerver AB",
    "license": "ISC",
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@storybook/addon-actions": "^5.0.5",
        "@storybook/addon-knobs": "^5.0.5",
        "@storybook/addon-links": "^5.0.5",
        "@storybook/addons": "^5.0.5",
        "@storybook/react": "^5.0.5",
        "@types/react": "^16.8.7",
        "@types/react-dom": "^16.8.2",
        "babel-loader": "^8.0.5",
        "css-loader": "^2.1.0",
        "husky": "^2.2.0",
        "jsdom": "^14.0.0",
        "lint-staged": "^8.1.6",
        "node-sass": "^4.11.0",
        "prettier": "1.16.1",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "ts-loader": "^5.3.3",
        "typescript": "^3.3.3333",
        "webpack": "^4.29.5",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.2.3"
    },
    "dependencies": {
        "@material/react-button": "^0.10.0",
        "@material/react-checkbox": "^0.10.0",
        "@material/react-dialog": "^0.10.0",
        "@material/react-icon-button": "^0.10.0",
        "@material/react-list": "^0.10.0",
        "@material/react-material-icon": "^0.10.0",
        "@material/react-menu-surface": "^0.8.0",
        "@material/react-snackbar": "^0.11.0",
        "@material/react-switch": "^0.11.0",
        "@material/react-text-field": "^0.10.0",
        "axios": "^0.18.0",
        "classnames": "^2.2.6",
        "date-fns": "^1.30.1",
        "html2canvas": "^1.0.0-alpha.12",
        "mobx": "^5.9.0",
        "mobx-react": "^5.4.3",
        "mobx-react-lite": "^1.2.0",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "react-image-crop": "^6.0.18",
        "react-scrollbar": "^0.5.6",
        "react-sidebar": "^3.0.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,scss,json,html,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
