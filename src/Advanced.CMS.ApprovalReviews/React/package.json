    "scripts": {
        "build": "yarn build:widget && yarn build:admin && yarn build:external-review-manage-links-widget && yarn build:editable-external-component",
        "build:debug": "yarn build:widget:debug && yarn build:admin && yarn build:external-review-manage-links-widget:debug && yarn build:editable-external-component",
        "build:widget": "webpack --config webpack.widget.config.js --mode production",
        "build:widget:debug": "webpack --config webpack.widget.config.js --mode development",
        "build:admin": "vite build --config vite-admin.config.ts",
        "check:types": "run -T tsc",
        "build:editable-external-component": "vite build --config vite-external.config.ts",
        "build:external-review-manage-links-widget": "webpack --config webpack.external-review-manage-links.widget.config --mode production",
        "build:external-review-manage-links-widget:debug": "webpack --config webpack.external-review-manage-links.widget.config --mode development",
        "storybook": "start-storybook -s ./ -p 6006",
        "build-storybook": "build-storybook",
        "test": "vitest"
    },
    "author": "advanced-cms",
    "license": "MIT",
    "private": true,
